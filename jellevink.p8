debug=false

function _init()
	colors()
	setplayerpos()
	setpath()
	genpath()
end

function _update()
	screen_shake() //to activate, change "offset" var

 frame+=1
	if status==0 then
	 updatemenu()
	elseif status==1 then
	 updategame()
	elseif status==2 then
	 updategover()
 end
end

function _draw()
 cls(0)
 if status==0 then
  drawmenu()
 elseif status==1 then
  drawgame()
 elseif status==2 then
  drawgover()
 end
end
