-- start menu

function updatemenu()
 if btnp(‚ùé) then
  setpath()
  setplayerpos()
 	trns=true
 end
end

function drawmenu()
 menucolors={1,5,0,5}
 linecolors={9,9,10,10}
 
 rectfill(0,57,128,71,9)
 
 textcolor=menucolors[flr(frame/6)%#menucolors+1]
 print("press ‚ùé to start",33.5,62,textcolor)

 for i=1,128 do
  linecolor=linecolors[(flr(frame/4)+i)%#linecolors+1]
  pset((frame+i*1)%128,72-sin(i/16)*0.5,linecolor) 
  pset((frame+i*1)%128,57+sin(i/16)*0.5,linecolor) 
 end
 sspr(0,16,8,8,frame*2%160-32,0,32,32)
 sspr(0,16,8,8,frame*-2%160-32,96,32,32,true)

 if trns==true then
  transition()
 end

end

t=4
function transition()
 t+=t/8
 rectfill(0,64+t,128,64-t,9)
 if t>64 then
  status=1
  frame=0
 end
end
