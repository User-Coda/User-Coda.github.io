-- draw

function drawgame()
 map(0,0,0,0,127,47) 
 
 drawmap()
 drawplayer()
 drawscore()
 
 if countdown==true then
  drawcountdown()
 end

  if debug then //enable debugging ui
  print("frame: "..frame,5,5,7)
  print("dx: "..dx)
  print("p: "..p)
  print("#path: "..#path)
  print("facing: "..facing)
  print("onpath: "..(onpath and 'true' or 'false'))
  print("score: "..score)
 end
end

function drawscore()
 width = #tostr(flr(score))*4+4
 rectfill(6,112,32+width,120,8)
 rect(6,112,32+width,120,14)
 print("score: "..flr(score),8,114,14)
end

function drawplayer()
 if facing==0 then
  spr(16,playerpos[1],playerpos[2])
 elseif facing==1 then
  spr(32,playerpos[1],playerpos[2])
 else
  spr(32,playerpos[1],playerpos[2],1,1,true)
 end
end

function drawmap() //draw all sprites from path table
  for v in all(path) do
  spr(v[3],v[1],v[2])
 end
end

function screen_shake()
 local fade = 0.95
 local offset_x=16-rnd(32)
 local offset_y=16-rnd(32)
 offset_x*=offset
 offset_y*=offset
 
 camera(offset_x,offset_y)
 offset*=fade
 if offset<0.05 then
   offset=0
 end
end

function drawcountdown()
 rectfill(1,1,frame*2,1,8)
end
