--map-functions

function movemap() //move the entire map and unload for player moving upwards
 score+=1/8/4
 foreach(path, function(obj)
  obj[2]+=1 //move map down 1
  if obj[2]>(16*8) then //remove path out of bound
   del(path,obj)
  end
 end)
end



function move(way) //move the path "snake", for generating
 if rnd() < 0.03 then
  sprite = 50
 else
  sprite = 1
 end
 
 if way=="up" then
  add(path,{path[#path][1],path[#path][2]-8,sprite})
  momentum="up"
 elseif way=="left" then
  add(path,{path[#path][1]-8,path[#path][2],sprite})
  momentum="left"
 elseif way=="right" then
  add(path,{path[#path][1]+8,path[#path][2],sprite})
  momentum="right"
 end
end



function genpath()
 dx=path[#path][1]-(8*8)
 p=abs(dx/4/8) //probability of going back to middle,
           //to prevent path going out of bounds
  
  
  
  //--=what direction to go=--
  
  if(dx>0) then //right side
   if rnd() < p then //move up or back to mid
    if rnd() < 0.5 and momentum != "right" then
     move("left")
    else
				 move("up")
				end
   elseif momentum != "left" then
    move("right")
   else
    move("up")
   end
   
  elseif dx<0 then //left side
   if rnd() < p then //move up or back to mid
    if rnd() < 0.5 and momentum != "left" then
     move("right")
    else
     move("up")
    end
   elseif momentum != "right" then
    move("left")
   else
    move("up")
   end
   
  else //middle
   if momentum != "right" then
    move("left")
   elseif momentum != "left" then
    move("right")
   else
    move("up")
   end
 end
  
end
