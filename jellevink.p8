-- update

function updategame()
 pathcheck()
 input()
 
 if frame>64 then
  moveplayer()
  countdown=false
 else
  countdown=true
 end
 
 //gen new path if neccesary
 while path[#path][2] > -8 do
 	genpath()
	end
 
end

nextfacing = 0
function input()
 if btnp(‚¨ÜÔ∏è) then nextfacing = 0 end
 if btnp(‚¨ÖÔ∏è) then nextfacing = -1 end
 if btnp(‚û°Ô∏è) then nextfacing = 1 end
  
 if (frame-1)%8==0 then //update facing when centered on sprite
  facing = nextfacing
 end
end

function moveplayer()
	 if facing==0 then
	  movemap()
	 elseif facing==1 then
	  playerpos[1]+=1
	 else
	  playerpos[1]-=1
	 end
end

k=0
function pathcheck()
 onpath = false
 foreach(path, function(obj)
 	if flr(playerpos[1]/8) == flr(obj[1]/8) and flr(playerpos[2]/8) == flr(obj[2]/8) then
  	if obj[3] == 50 then
    colors() //update colors
    sfx(0)
    offset=0.125
    score+=4
   else
    offset=0
   end
  	
  	onpath = true
  	obj[3] = 49
  end
 end)
 
 if onpath==false then
  sfx(2)
  offset=0.125
  colors()
  frame=0
  k+=1
  if(k>30) then
   status=2
   sfx(1)
   offset=0.125
  end
 end
end
