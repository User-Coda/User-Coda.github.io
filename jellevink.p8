-- data

status=0 //0=menu,1=game,2=gover
frame=0
momentum="up" //defines previous path direction
trns=false
colliding=false
score=0
offset=0
countdown=false //start countdown
facing = 0 //-1=left, 0=up, 1=right


function setpath()
path = {
 {8*8,14*8,1},
 {8*8,13*8,1},
 {8*8,12*8,1},
 {8*8,11*8,1},
 {8*8,10*8,1}
}//x, y, sprite
end

function setplayerpos()
 playerpos = {8*8,10*8}
end

col=1
function colors() //update colour pallette
 
 cm={
 {0,192,1,140,12,5,6,7,
 8,139,138,138,11,139,14,13},
 {0,192,1,140,12,5,6,7,
 8,9,10,138,11,139,14,13},
 {0,192,1,140,12,5,6,7,
 8,140,12,138,11,139,14,13}
 }
	
	for i=0,15 do
	 pal(i,cm[col%3+1][i+1],1)
	end
	poke(0x5f2e,1 )
	
	col+=1
end








