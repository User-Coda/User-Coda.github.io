--gameover
function updategover()
 if btnp(‚ùé) then //restart
  onpath = true
  score=0
  frame=0
  nextfacing=0
  facing=0
  setpath()
  setplayerpos()
  status=1
 end
end

function drawgover()
 
 print("score: "..score,8,8,8)
 print("game over!",45,32,9)
 print("press ‚ùé to restart",25,39,10)
 
 size = flr(abs(sin(frame/80))*6+16/8)*8
 sspr(0,8,8,8,64-size/2,100-size/2,size,size)
end
